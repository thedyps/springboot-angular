<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.thedyps.springbootangular.member.MemberDAO">
    <select id="selectById" parameterType="String"
            resultType="xyz.thedyps.springbootangular.member.memberVO.MemberVO">
        select * from MEMBER where id = #{id,javaType=String,jdbcType=VARCHAR}
    </select>
</mapper>